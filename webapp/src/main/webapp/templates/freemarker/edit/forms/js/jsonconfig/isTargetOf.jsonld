{
  "@context": {
    "foaf": "http://xmlns.com/foaf/0.1/",
    "forms": "java:edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.forms#",
    "bib": "http://bibliotek-o.org/ontology/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "vivo": "http://vivoweb.org/ontology/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "bibframe": "http://id.loc.gov/ontologies/bibframe/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "oa": "http://www.w3.org/ns/oa#",
    "customform": "http://vitro.mannlib.cornell.edu/ns/vitro/CustomFormConfiguration#"
  },
  "@graph": [
    {
      "@id": "customform:addAnnotation_requiredN3",
      "@type": [
        "forms:RequiredN3Pattern",
        "forms:FormComponent"
      ],
      "customform:pattern": [
      	"?subject bib:isTargetOf ?object.",
		"?object rdf:type oa:Annotation .",
		"?object oa:hasTarget ?subject. ",
		"?object oa:hasBody ?bodyUri .",
		"?bodyUri rdf:type oa:TextualBody.",
		"?bodyUri oa:bodyValue ?bodyValue.",
       	"?bodyUri rdf:value ?bodyValue."
      ],
      "customform:prefixes": "@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . @prefix bib: <http://bibliotek-o.org/ontology/> . @prefix bibframe: <http://id.loc.gov/ontologies/bibframe/> .   @prefix  rdfs: <http://www.w3.org/2000/01/rdf-schema#>  .   @prefix dcterms: <http://purl.org/dc/terms/> .  @prefix lexvo: <http://lexvo.org/id/iso639-3/> . @prefix owl: <http://www.w3.org/2002/07/owl#> . @prefix oa: <http://www.w3.org/ns/oa#> ."
    },
    {
      "@id": "customform:addWork_optionalN3",
      "@type": [
        "forms:OptionalN3Pattern",
        "forms:FormComponent"
      ],
      "customform:pattern": [
       "?object oa:motivatedBy ?motivationIndividual.",
       "?object dcterms:creator ?creatorUri.",
       "?bodyUri oa:hasSource ?sourceUri.",
       "?bodyUri oa:hasSelector ?selectorUri.",
       "?selectorUri rdf:type ?selectorUriType .",
       "?selectorUri rdf:type oa:Selector."      ],
      "customform:prefixes": "@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . @prefix bib: <http://bibliotek-o.org/ontology/> . @prefix bibframe: <http://id.loc.gov/ontologies/bibframe/> .   @prefix  rdfs: <http://www.w3.org/2000/01/rdf-schema#>  .   @prefix dcterms: <http://purl.org/dc/terms/> .  @prefix foaf: <http://xmlns.com/foaf/0.1/> . @prefix lexvo: <http://lexvo.org/id/iso639-3/> . @prefix owl: <http://www.w3.org/2002/07/owl#> ."
    },
  
    {
      "@id": "customform:whcor_bodyUri",
      "@type": [
       "forms:FormComponent",
       "forms:UriField"
      ],
      "customform:mayUseNewResource": true,
      "customform:varName": "bodyUri"
    },
    {
      "@id": "customform:whcor_bodyValue",
      "@type": [
       "forms:FormComponent",
       "forms:LiteralField"
      ],
      "customform:varName": "bodyValue"
    },
     {
      "@id": "customform:whcor_motivation",
      "@type": [
       "forms:FormComponent",
       "forms:UriField"
      ],
      "customform:varName": "motivation",
       "customform:fieldOptions": {
        "@id": "customform:whcor_motivationFieldOptions"
      }
    },
     {
      "@id": "customform:whcor_motivationFieldOptions",
      "@type": [
        "forms:IndividualsViaVClassOptions",
        "forms:FieldOptions"
      ],
      "customform:classUri": {
        "@id": "http://www.w3.org/ns/oa#Motivation"
      }
    }
    
   ]
}