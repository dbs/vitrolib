{
  "@context": {
    "foaf": "http://xmlns.com/foaf/0.1/",
    "forms": "java:edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.forms#",
    "ld4l": "http://bib.ld4l.org/ontology/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "vivo": "http://vivoweb.org/ontology/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "bibframe": "http://id.loc.gov/ontologies/bibframe/",
    "customform": "http://vitro.mannlib.cornell.edu/ns/vitro/CustomFormConfiguration#"
  },
  "@graph": [
    {
      "@id": "customform:whcor_requiredN3",
      "@type": [
        "forms:RequiredN3Pattern",
        "forms:FormComponent"
      ],
      "customform:pattern": [
        "?objectVar ld4l:hasAgent ?agent . ?agent ld4l:isAgentOf ?objectVar .    ",
        "?objectVar a ld4l:Activity . ?objectVar a ?activityType . ?objectVar rdfs:label ?activityLabel .     ",
        "?subject ld4l:hasActivity ?objectVar . ?objectVar ld4l:isActivityOf ?subject .    "
      ],
      "customform:prefixes": "@prefix ld4l: <http://bib.ld4l.org/ontology/> . @prefix bibframe: <http://id.loc.gov/ontologies/bibframe/> .   @prefix  rdfs: <http://www.w3.org/2000/01/rdf-schema#>  .     "
    },
    {
      "@id": "customform:whcor_fieldAgent",
      "@type": [
        "forms:FormComponent",
        "forms:UriField"
      ],
      "customform:mayUseNewResource": true,
      "customform:queryForExistingValue": "PREFIX ld4l: <http://bib.ld4l.org/ontology/>   SELECT ?existingAgent WHERE {  ?objectVar ld4l:hasAgent ?existingAgent . }   ",
      "customform:validator": {
        "@id": "customform:whcor_nonemptyValidator"
      },
      "customform:varName": "agent"
    },
    {
      "@id": "customform:whcor_optionalN3",
      "@type": [
        "forms:FormComponent",
        "forms:OptionalN3Pattern"
      ],
      "customform:prefixes": "@prefix  ld4l: <http://bib.ld4l.org/ontology/> . @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>  . @prefix  foaf: <http://xmlns.com/foaf/0.1/> .  ",
      "customform:pattern": ["?agent a ?agentType.  ?agent rdfs:label ?agentName.  ?agent foaf:name ?agentName .  "]
    },
    {
      "@id": "customform:whcor_fieldActivityLabel",
      "@type": [
        "forms:LiteralField",
        "forms:FormComponent"
      ],
      "customform:queryForExistingValue": "PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>   PREFIX ld4l: <http://bib.ld4l.org/ontology/>      SELECT ?existingActivityLabel     WHERE {?objectVar rdfs:label ?existingActivityLabel .}  ",
      "customform:rangeDatatypeUri": {
        "@id": "xsd:string"
      },
      "customform:varName": "activityLabel"
    },
    {
      "@id": "customform:whcor_nonemptyValidator",
      "@type": [
        "forms:NonEmptyFieldValidator",
        "forms:FieldValidator"
      ]
    },
    {
      "@id": "customform:whcor_fieldAgentName",
      "@type": [
        "forms:LiteralField",
        "forms:FormComponent"
      ],
      "customform:queryForExistingValue": "PREFIX ld4l: <http://bib.ld4l.org/ontology/>  PREFIX prov: <http://www.w3.org/ns/prov#>    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>  PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>   SELECT ?existingAgentName   WHERE {?objectVar prov:agent ?agent .?agent rdfs:label ?existingAgentName . }    ",
      "customform:varName": "agentName"
    },
    {
      "@id": "customform:whcor_agentListFieldOptions",
      "@type": [
        "forms:ChildVClassesWithParent",
        "forms:FieldOptions"
      ],
      "customform:classUri": {
        "@id": "http://xmlns.com/foaf/0.1/Agent"
      }
    },
    {
      "@id": "customform:whcor_fieldAgentType",
      "@type": [
        "forms:UriField",
        "forms:FormComponent"
      ],
      "customform:fieldOptions": {
        "@id": "customform:whcor_agentListFieldOptions"
      },
      "customform:queryForExistingValue": "PREFIX ld4l: <http://bib.ld4l.org/ontology/>  PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>     SELECT ?existingAgentType   WHERE {?objectVar ld4l:hasAgent ?agent . ?agent vitro:mostSpecificType ?existingAgentType .}    ",
      "customform:validator": {
        "@id": "customform:whcor_nonemptyValidator"
      },
      "customform:varName": "agentType"
    },
    {
      "@id": "customform:whcor_activityTypeListFieldOptions",
      "@type": [
        "forms:ChildVClassesWithParent",
        "forms:FieldOptions"
      ],
      "customform:classUri": {
        "@id": "http://bib.ld4l.org/ontology/Activity"
      }
    },
    {
      "@id": "customform:workHasActivity",
      "@type": [
        "forms:FormBuilder",
        "forms:SimpleFormBuilder"
      ],
      "customform:component": [
        {
          "@id": "customform:whcor_requiredN3"
        },
        {
          "@id": "customform:whcor_fieldAgent"
        },
        {
          "@id": "customform:whcor_fieldAgentName"
        },
        {
          "@id": "customform:whcor_fieldActivityType"
        },
        {
          "@id": "customform:whcor_fieldActivityLabel"
        },
        {
          "@id": "customform:whcor_optionalN3"
        },
        {
          "@id": "customform:whcor_fieldAgentType"
        }
      ],
      "customform:returnTo": "HERE"
    },
    {
      "@id": "customform:whcor_fieldActivityType",
      "@type": [
        "forms:FormComponent",
        "forms:UriField"
      ],
      "customform:fieldOptions": {
        "@id": "customform:whcor_activityTypeListFieldOptions"
      },
      "customform:queryForExistingValue": "PREFIX ld4l: <http://bib.ld4l.org/ontology/>     PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>      SELECT ?existingActivityType      WHERE {?objectVar vitro:mostSpecificType ?existingActivityType .   }    ",
      "customform:validator": {
        "@id": "customform:whcor_nonemptyValidator"
      },
      "customform:varName": "activityType"
    }
  ]
}