<?xml version="1.0" encoding="UTF-8"?>

<!-- TODO Add/remove as needed -->
<rdf:RDF  
    xmlns="http://bib.ld4l.org/ontology/"
    xmlns:bf="http://id.loc.gov/ontologies/bibframe/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:ld4l="http://bib.ld4l.org/ontology/"
    xmlns:oa="http://www.w3.org/ns/oa#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"  
    xmlns:schema="http://schema.org/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    >
    
    
    <!-- ONTOLOGY DECLARATION -->
       
    <owl:Ontology rdf:about="http://bib.ld4l.org/ontology/">
        <owl:ontologyIRI rdf:resource="http://bib.ld4l.org/ontology"/>
        <!-- TODO Need redirect from the version IRI to the ontology IRI -->
        <!-- TODO Fill in date of publication -->       
        <owl:versionIRI rdf:resource="http://bib.ld4l.org/2017/02/10/ontology"/>
        <!-- TODO Fill in datetime of publication -->
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2017-02-10T15:31:13.103-04:00</owl:versionInfo>
        <!-- TODO Fill in date of publication -->
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
        <rdfs:label xml:lang="en">LD4L BIBFRAME 2 Extension</rdfs:label>
        <rdfs:comment xml:lang="en">This ontology fragment includes extensions and modifications to BIBFRAME 2 and is intended to be used as a supplement to the core BIBFRAME 2 ontology. It is a joint product of the Mellon Foundation-funded Linked Data for Libraries - Labs and the Linked Data for Production projects. See http://ld4l.org for more information on these projects.</rdfs:comment>
        <vann:preferredNamespacePrefix>ld4l</vann:preferredNamespacePrefix>
    </owl:Ontology>
    
 
 
    <!-- CLASSES -->

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AbbreviatedTitle">
        <rdfs:label xml:lang="en">Abbreviated title</rdfs:label>
        <skos:definition xml:lang="en">Title as abbreviated for citation, indexing, and/or identification.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AcquisitionActivity">
        <rdfs:label xml:lang="en">Acquisition</rdfs:label>    
        <skos:definition xml:lang="en">The activity of acquiring a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Activity">
        <rdfs:label xml:lang="en">Activity</rdfs:label>    
        <skos:definition xml:lang="en">An activity or contribution by a single agent that affects or alters the existence or state of a resource.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AuthorActivity">
        <rdfs:label xml:lang="en">Author</rdfs:label>    
        <skos:definition xml:lang="en">The activity of authoring a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CaptureActivity">
        <rdfs:label xml:lang="en">Capture</rdfs:label>    
        <skos:definition xml:lang="en">The recording, filming, etc. of a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ComposerActivity">
        <rdfs:label xml:lang="en">Composer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of composing a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ConciseTitle">
        <rdfs:label xml:lang="en">Concise title</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <skos:example xml:lang="en">&apos;Modern Writing&apos; could serve as the concise title for &apos;The Berkley Book of Modern Writing&apos;.</skos:example>
        <skos:definition xml:lang="en">The concise version of a title.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ConductorActivity">
        <rdfs:label xml:lang="en">Conductor</rdfs:label>    
        <skos:definition xml:lang="en">The activity of conducting a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CreatorActivity">
        <rdfs:label xml:lang="en">Creator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of creating a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Credits">
        <rdfs:label xml:lang="en">Credits</rdfs:label>
        <skos:definition xml:lang="en">Credits for agents who have participated in the creation and/or production of a resource.</skos:definition>
        <skos:scopeNote xml:lang="en">Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CustodialHistory">
        <rdfs:label xml:lang="en">Custodial history</rdfs:label>
        <skos:definition xml:lang="en">Information about the provenance, such as origin, ownership and custodial history (chain of custody), of a resource.</skos:definition>
        <skos:scopeNote xml:lang="en">Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/DistinctiveTitle">
        <rdfs:label xml:lang="en">Distinctive title</rdfs:label>
        <skos:definition xml:lang="en">Special title that appears in addition to the regular title on individual issues of an item and by which the issue may be known.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>   
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/DistributorActivity">
        <rdfs:label xml:lang="en">Distributor</rdfs:label>    
        <skos:definition xml:lang="en">The activity of distributing a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/EditorActivity">
        <rdfs:label xml:lang="en">Editor</rdfs:label>    
        <skos:definition xml:lang="en">The activity of editing a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/HistoryOfWork">
        <rdfs:label xml:lang="en">History of work</rdfs:label>
        <skos:definition xml:lang="en">Information about the history of a work.</skos:definition>
        <skos:scopeNote xml:lang="en">Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/IllustratorActivity">
        <rdfs:label xml:lang="en">Illustrator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of illustrating a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/KeyTitle">
        <rdfs:label xml:lang="en">Key title</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <skos:definition xml:lang="en">Unique title for a continuing resource that is assigned by the ISSN International Center in conjunction with an ISSN.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>


    <owl:Class rdf:about="http://bib.ld4l.org/ontology/MainTitleElement">
        <rdfs:label xml:lang="en">Main title element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition xml:lang="en">The main part of a title.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>    

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ManufacturerActivity">
        <rdfs:label xml:lang="en">Manufacturer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of manufacturing a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NarratorActivity">
        <rdfs:label xml:lang="en">Narrator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of narrating a  resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NatureOfContent">
        <rdfs:label xml:lang="en">Nature of content</rdfs:label>
        <skos:definition xml:lang="en">Description of the primary type of content of a resource.</skos:definition>
        <skos:example xml:lang="en">Field recording of birdsong; combined time series analysis and graph plotting system.</skos:example>
        <skos:scopeNote xml:lang="en">Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
 
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NonSortElement">
        <rdfs:label xml:lang="en">Non-sort element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition xml:lang="en">The initial segment of a title that is removed for sorting.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>       
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ParallelTitle">
        <rdfs:label xml:lang="en">Parallel title</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <skos:definition xml:lang="en">A title in another language.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PartNameElement">
        <rdfs:label xml:lang="en">Part name element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition xml:lang="en">The part of a title that specifies the name of a part of the resource.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>       
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PartNumberElement">
        <rdfs:label xml:lang="en">Part number element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition xml:lang="en">The part of a title that specifies the number of a part of the resource.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PerformerActivity">
        <rdfs:label xml:lang="en">Performer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of performing in a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ProducerActivity">
        <rdfs:label xml:lang="en">Producer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of producing a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PublisherActivity">
        <rdfs:label xml:lang="en">Publishing</rdfs:label>    
        <skos:definition xml:lang="en">The activity of publishing a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Software">
        <rdfs:label xml:lang="en">Software</rdfs:label>    
        <skos:definition xml:lang="en">A compiled executable binary file.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/SubtitleElement">
        <rdfs:label xml:lang="en">Subtitle element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition xml:lang="en">The subtitle of a title.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>   

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/SystemRequirements">
        <rdfs:label xml:lang="en">System requirements</rdfs:label>
        <skos:definition xml:lang="en">Description of the equipment or system requirements beyond what is normal and obvious for the type of carrier or type of file, such as make and model of equipment or hardware, operating system, amount of memory, programming language, other necessary software, any plug-ins or peripherals required to play, view, or run the resource, etc.</skos:definition>
        <skos:scopeNote xml:lang="en">Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
 
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Tactile">
        <rdfs:label xml:lang="en">Tactile</rdfs:label>    
        <skos:definition xml:lang="en">A work intended to be at least partially perceived with the sense of touch.</skos:definition>
        <skos:example xml:lang="en">The children's book Pat the Bunny.</skos:example>
        <skos:scopeNote xml:lang="en">Contrasts with http://id.loc.gov/ontologies/bibframe/TactileNotation, a notation used for transcribing a resource, such as braille.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/TitleElement">
        <rdfs:label xml:lang="en">Title element</rdfs:label>    
        <skos:definition xml:lang="en">A part of a title, such as a subtitle, part number, or part name.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>    

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/TitleOrigin">
        <rdfs:label xml:lang="en">Title origin</rdfs:label>    
        <skos:definition xml:lang="en">The place from which a title originates, such as spine, cover, container, etc.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://bib.ld4l.org/ontology/TranslatorActivity">
        <rdfs:label xml:lang="en">Translator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of translatin a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>    



    <!-- OBJECT PROPERTIES --> 


    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/atLocation">
        <rdfs:label xml:lang="en">at location</rdfs:label>
        <skos:definition xml:lang="en">The resource being described is at the specified location.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/locationOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasActivity">
        <rdfs:label xml:lang="en">has activity</rdfs:label>
        <skos:definition xml:lang="en">Relates this resource to an activity or contribution by a single agent that affects or alters its existence or state.</skos:definition>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isActivityOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasAgent">
        <rdfs:label xml:lang="en">has agent</rdfs:label>
        <skos:definition xml:lang="en">Relates an activity to its agent.</skos:definition>
        <rdfs:domain rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isAgentOf"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasAnnotation">
        <rdfs:label xml:lang="en">has annotation</rdfs:label>
        <skos:definition xml:lang="en">Relates a resource to its annotation.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Annotation"/> 
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/oa#hasTarget"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasGeographicCoverage">
        <rdfs:label xml:lang="en">has geographic coverage</rdfs:label>
        <skos:definition xml:lang="en">Specifies the geographic coverage of the content of the resource.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/> 
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasOrigin">
        <rdfs:label xml:lang="en">has origin</rdfs:label>    
        <skos:definition xml:lang="en">Specifies the physical location from which this title originates, such as spine, cover, container, etc.</skos:definition>
        <skos:scopeNote xml:lang="en">Distinct from http://bib.ld4l.org/ontology/hasSource, which specifies an external organization or scheme from which the title was obtained.</skos:scopeNote>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/TitleOrigin"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasPreferredTitle">
        <rdfs:label xml:lang="en">has preferred title</rdfs:label>
        <skos:definition xml:lang="en">Specifies the preferred title of this resource.</skos:definition>
        <skos:scopeNote xml:lang="en">A resource may have multiple directly linked titles, among which this is the preferred title.</skos:scopeNote>
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/title"/>
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isPreferredTitleOf"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasSource">
        <rdfs:label xml:lang="en">has source</rdfs:label>
        <skos:definition xml:lang="en">Relates this resource to the source from which it was derived.</skos:definition>
        <skos:scopeNote xml:lang="en">Has general applicability to many types of sources and resources.</skos:scopeNote>
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isSourceOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasTemporalCoverage">
        <rdfs:label xml:lang="en">has temporal coverage</rdfs:label>
        <skos:definition xml:lang="en">Specifies the time period covered in the content of the resource.</skos:definition>
