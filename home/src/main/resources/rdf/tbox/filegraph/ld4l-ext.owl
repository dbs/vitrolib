<rdf:RDF  
    xmlns="http://bib.ld4l.org/ontology/"
    xmlns:bf="http://id.loc.gov/ontologies/bibframe/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:ld4l="http://bib.ld4l.org/ontology/"
    xmlns:oa="http://www.w3.org/ns/oa#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"  
    xmlns:schema="http://schema.org/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    >
    
    <owl:Ontology rdf:about="http://bib.ld4l.org/ontology/">
        <owl:ontologyIRI rdf:resource="http://bib.ld4l.org/ontology"/>
        <!-- TODO Need redirect from the version IRI to the ontology IRI -->
        <!-- TODO Fill in date of publication -->       
        <owl:versionIRI rdf:resource="http://bib.ld4l.org/2017/02/10/ontology"/>
        <!-- TODO Fill in datetime of publication -->
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2017-02-10T15:31:13.103-04:00</owl:versionInfo>
        <!-- TODO Fill in date of publication -->
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
        <!-- TODO Fill in ontology definition (rdfs:comment / skos:definition?). Extension of bf2 core, some additions, some mods. -->
        <vann:preferredNamespacePrefix>ld4l</vann:preferredNamespacePrefix>
    </owl:Ontology>
    
    
    <!-- CLASSES -->
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ConciseTitle">
        <rdfs:label>Concise title</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <skos:example>&apos;Modern Writing&apos; could serve as the concise title for &apos;The Berkley Book of Modern Writing&apos;.</skos:example>
        <skos:definition xml:lang="en-us">The concise version of a title.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/KeyTitle">
        <rdfs:label>Key title</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <skos:definition xml:lang="en-us">Unique title for a continuing resource that is assigned by the ISSN International Center in conjunction with an ISSN.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ParallelTitle">
        <rdfs:label>Parallel title</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <skos:definition xml:lang="en-us">A title in another language.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/DistinctiveTitle">
        <rdfs:label>Distinctive title</rdfs:label>
        <skos:definition>Special title that appears in addition to the regular title on individual issues of an item and by which the issue may be known.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AbbreviatedTitle">
        <rdfs:label>Abbreviated title</rdfs:label>
        <skos:definition>Title as abbreviated for citation, indexing, and/or identification.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Credits">
        <rdfs:label>Credits</rdfs:label>
        <skos:definition>Credits for agents who have participated in the creation and/or production of a resource.</skos:definition>
        <skos:scopeNote>Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CustodialHistory">
        <rdfs:label>Custodial history</rdfs:label>
        <skos:definition>Information about the provenance, such as origin, ownership and custodial history (chain of custody), of a resource.</skos:definition>
        <skos:scopeNote>Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/HistoryOfWork">
        <rdfs:label>History of work</rdfs:label>
        <skos:definition>Information about the history of a work.</skos:definition>
        <skos:scopeNote>Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/SystemRequirements">
        <rdfs:label>System requirements</rdfs:label>
        <skos:definition>Description of the equipment or system requirements beyond what is normal and obvious for the type of carrier or type of file, such as make and model of equipment or hardware, operating system, amount of memory, programming language, other necessary software, any plug-ins or peripherals required to play, view, or run the resource, etc.</skos:definition>
        <skos:scopeNote>Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NatureOfContent">
        <rdfs:label>Nature of content</rdfs:label>
        <skos:definition>Description of the primary type of content of a resource.</skos:definition>
        <skos:example>Field recording of birdsong; combined time series analysis and graph plotting system.</skos:example>
        <skos:scopeNote>Used to type Annotation bodies. Recommended use with motivation http://bib.ld4l.org/ontology/cataloging.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/TitleElement">
        <rdfs:label>Title element</rdfs:label>    
        <skos:definition>A part of a title, such as a subtitle, part number, or part name.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>    
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/MainTitleElement">
        <rdfs:label>Main title element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The main part of a title.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>    
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NonSortElement">
        <rdfs:label>Non-sort element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The initial segment of a title that is removed for sorting.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>       
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PartNameElement">
        <rdfs:label>Part name element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The part of a title that specifies the name of a part of the resource.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>       
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PartNumberElement">
        <rdfs:label>Part number element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The part of a title that specifies the number of a part of the resource.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>      
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/SubtitleElement">
        <rdfs:label>Subtitle element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The subtitle of a title.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>       

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/TitleOrigin">
        <rdfs:label>Title origin</rdfs:label>    
        <skos:definition>The place from which a title originates, such as spine, cover, container, etc.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Software">
        <rdfs:label>Software</rdfs:label>    
        <skos:definition>A compiled executable binary file.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Activity">
        <rdfs:label>Activity</rdfs:label>    
        <skos:definition>An activity or contribution by a single agent that affects or alters the existence or state of a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AuthorActivity">
        <rdfs:label>Author</rdfs:label>    
        <skos:definition>The activity of authoring a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AcquisitionActivity">
        <rdfs:label>Acquisition</rdfs:label>    
        <skos:definition>The activity of acquiring a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/IllustratorActivity">
        <rdfs:label>Illustrator</rdfs:label>    
        <skos:definition>The activity of illustrating a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/EditorActivity">
        <rdfs:label>Editor</rdfs:label>    
        <skos:definition>The activity of editing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CreatorActivity">
        <rdfs:label>Creator</rdfs:label>    
        <skos:definition>The activity of creating a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ComposerActivity">
        <rdfs:label>Composer</rdfs:label>    
        <skos:definition>The activity of composing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/TranslatorActivity">
        <rdfs:label>Translator</rdfs:label>    
        <skos:definition>The activity of translatin a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ConductorActivity">
        <rdfs:label>Conductor</rdfs:label>    
        <skos:definition>The activity of conducting a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NarratorActivity">
        <rdfs:label>Narrator</rdfs:label>    
        <skos:definition>The activity of narrating a  resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PerformerActivity">
        <rdfs:label>Performer</rdfs:label>    
        <skos:definition>The activity of performing in a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/DistributorActivity">
        <rdfs:label>Distributor</rdfs:label>    
        <skos:definition>The activity of distributing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ManufacturerActivity">
        <rdfs:label>Manufacturer</rdfs:label>    
        <skos:definition>The activity of manufacturing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PublisherActivity">
        <rdfs:label>Publishing</rdfs:label>    
        <skos:definition>The activity of publishing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ProducerActivity">
        <rdfs:label>Producer</rdfs:label>    
        <skos:definition>The activity of producing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CaptureActivity">
        <rdfs:label>Capture</rdfs:label>    
        <skos:definition>The recording, filming, etc. of a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Braille">
        <rdfs:label>Braille</rdfs:label>    
        <skos:definition>A tactile notation used by the blind and visually impaired, based on a system of rectangular cells and raised dots.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://id.loc.gov/ontologies/bibframe/TactileNotation"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Tactile">
        <rdfs:label>Tactile</rdfs:label>    
        <skos:definition>A work intended to be at least partially perceived with the sense of touch.</skos:definition>
        <skos:example>The children's book Pat the Bunny.</skos:example>
        <skos:scopeNote>Contrasts with http://id.loc.gov/ontologies/bibframe/TactileNotation, a notation used for transcribing a resource, such as braille.</skos:scopeNote>
        <rdfs:subClassOf rdfs:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    
    <!-- OBJECT PROPERTIES --> 
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasOrigin">
        <rdfs:label>has origin</rdfs:label>    
        <skos:definition>Specifies the physical location from which this title originates, such as spine, cover, container, etc.</skos:definition>
        <skos:scopeNote>Distinct from http://bib.ld4l.org/ontology/hasSource, which specifies an external organization or scheme from which the title was obtained.</skos:scopeNote>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/TitleOrigin"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasActivity">
        <rdfs:label>has activity</rdfs:label>
        <skos:definition>Relates this resource to an activity or contribution by a single agent that affects or alters its existence or state.</skos:definition>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isActivityOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/geographicCoverage">
        <rdfs:label>geographic coverage</rdfs:label>
        <skos:definition>Specifies the geographic coverage of the content of the resource.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/> 
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/temporalCoverage">
        <rdfs:label>geographic coverage</rdfs:label>
        <skos:definition>Specifies the time period covered in the content of the resource.</skos:definition>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Temporal"/> 
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/isActivityOf">
        <rdfs:label>has activity</rdfs:label>
        <skos:definition>Relates an activity to the affected resource.</skos:definition>
        <rdfs:domain rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/hasActivity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasAnnotation">
        <rdfs:label>has annotation</rdfs:label>
        <skos:definition>Relates a resource to its annotation.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Annotation"/> 
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/oa#hasTarget"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasAgent">
        <rdfs:label>has agent</rdfs:label>
        <skos:definition>Relates an activity to its agent.</skos:definition>
        <rdfs:domain rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isAgentOf"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/isAgentOf">
        <rdfs:label>is agent of</rdfs:label>
        <skos:definition>Relates an agent to the activity it participated in.</skos:definition>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/hasAgent"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasSource">
        <rdfs:label>has source</rdfs:label>
        <skos:definition>Relates this resource to the source from which it was derived.</skos:definition>
        <skos:scopeNote>Has general applicability to many types of sources and resources.</skos:scopeNote>
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isSourceOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/isSourceOf">
        <rdfs:label>is source of</rdfs:label>
        <skos:definition>Relates this resource to a resource of which it is the source.</skos:definition>
        <skos:scopeNote>Has general applicability to many types of sources and resources.</skos:scopeNote>        
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/hasSource"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasPreferredTitle">
        <rdfs:label xml:lang="en-us">has preferred title</rdfs:label>
        <skos:definition>Specifies the preferred title of this resource.</skos:definition>
